{
    "manifest_version": 3,
    "name": "Tag Generator",
    "version": "1.0",
    "description": "Тегирование заявок 2ЛТП.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
      }
    },
    "permissions": [
      "clipboardWrite",
      "notifications",
      "storage"
    ]
  }